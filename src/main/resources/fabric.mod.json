{
	"schemaVersion": 1,
	"id": "phonochat",
	"version": "${version}",
	"name": "Phonochat",
	"description": "A basic Phonos / Simple Voice Chat integration mod. Adds the microphone item which allows you to broadcast your voice to a speaker.",
	"authors": [
		"Sylv"
	],
	"contact": {
		"homepage": "https://sylv.gay/",
		"sources": "https://github.com/SylvKT/phonochat"
	},
	"license": "MIT",
	"icon": "assets/phonochat/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"gay.sylv.phonochat.PhonochatMod"
		],
		"client": [
			"gay.sylv.phonochat.client.PhonochatClient"
		],
		"voicechat": [
			"gay.sylv.phonochat.plugin.PhonochatVoicechatPlugin"
		]
	},
	"mixins": [
		"phonochat.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loaderVersion}",
		"fabric": ">=${fabricVersion}",
		"minecraft": "${minecraftDependency}",
		"java": ">=${javaVersion}",
		"voicechat": ">=${voicechatDependency}",
		"phonos": ">=${phonosDependency}",
		"trinkets": ">=${trinketsDependency}"
	}
}
